{
    "$schema": "http://json-schema.org/schema",
    "id": "SchematicsForService",
    "title": "JWT Service Schema",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "default": "name"
      },
      "path": {
        "type": "string",
        "default": "app"
      },
      "appRoot": {
        "type": "string"
      },
      "sourceDir": {
        "type": "string",
